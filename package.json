{
  "name": "react-rezoom.js",
  "version": "1.0.1",
  "main": "dist/cjs/ImageZoom.js",
  "module": "dist/vite/ImageZoom.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run postbuild",
    "postbuild": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc ",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "keywords": [
    "reactimagezoomimage",
    "magnifierimage",
    "enlargerimage",
    "lensphoto",
    "zoomphoto",
    "magnifierphoto",
    "enlargerphoto",
    "lensimage",
    "zoomimag",
    "magnifierimage",
    "enlargerphoto",
    "zoomphoto",
    "magnifierphoto",
    "enlargerzoommagnifyenlargereact-image-zoomreact-photo-zoom"
  ],
  "description": "React component for desktop browsers for image zoom on mouse hover",
  "repository": {
    "type": "git",
    "url": "https://github.com/lavishabhatia/react-rezoom.js"
  },
  "author": "Lavisha Bhatia <lavishabhatia1012@gmail.com> (https://github.com/lavishabhatia)",
